   "id": "be3206b7-1ff1-47b0-911f-cf3637e28b3b",
   "metadata": {},
   "outputs": [],
   "source": [
    "def getManhattanDistance(pos, basePos):\n",
    "    return abs(pos[0] - basePos[0]) + abs(pos[1] - basePos[1])"
   ]
  }
